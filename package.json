	"license": "MIT",
	"devDependencies": {
		"@types/acorn": "^6.0.4",
		"@types/estree": "^1.0.6",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.13.10",
		"@typescript-eslint/eslint-plugin": "^8.26.1",
		"@typescript-eslint/parser": "^8.26.1",
		"builtin-modules": "^5.0.0",
		"esbuild": "^0.25.1",
		"jest": "^29.7.0",
		"obsidian": "^1.8.7",
		"ts-jest": "^29.2.6",
		"tslib": "^2.8.1",
		"typescript": "^5.8.2"
	},
	"dependencies": {
		"@hypersphere/sqlseal": "^0.32.0",
